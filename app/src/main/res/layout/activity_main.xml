<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/wajah"
        android:layout_width="182dp"
        android:layout_height="361dp"
        android:src="@drawable/body"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.207"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/rambut"
        android:layout_width="292dp"
        android:layout_height="104dp"
        android:src="@drawable/hair"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.052"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/mata"
        android:layout_width="289dp"
        android:layout_height="81dp"
        android:layout_marginBottom="4dp"
        android:src="@drawable/eyes"
        android:visibility="visible"
        app:layout_constraintBottom_toTopOf="@+id/imageView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.877" />

    <ImageView
        android:id="@+id/alis"
        android:layout_width="156dp"
        android:layout_height="54dp"
        android:layout_marginTop="152dp"
        android:src="@drawable/eyebrow"
        android:visibility="visible"
        app:layout_constraintBottom_toTopOf="@+id/mata"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.539"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/janggut"
        android:layout_width="180dp"
        android:layout_height="114dp"
        android:src="@drawable/beard"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="@+id/wajah"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.969"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="258dp"
        android:layout_height="76dp"
        android:src="@drawable/moustache"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="@+id/janggut"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.503"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.763"
        tools:ignore="MissingConstraints" />

    <CheckBox
        android:id="@+id/cbrambut"
        android:layout_width="115dp"
        android:layout_height="42dp"
        android:layout_marginStart="60dp"
        android:layout_marginLeft="60dp"
        android:layout_marginTop="60dp"
        android:checked="true"
        android:text="Rambut"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/wajah"
        tools:ignore="MissingConstraints" />

    <CheckBox
        android:id="@+id/cbalis"
        android:layout_width="115dp"
        android:layout_height="42dp"
        android:layout_marginStart="60dp"
        android:layout_marginLeft="60dp"
        android:layout_marginTop="40dp"
        android:checked="true"
        android:text="Alis"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cbrambut"
        tools:ignore="MissingConstraints" />

    <CheckBox
        android:id="@+id/cbmata"
        android:layout_width="115dp"
        android:layout_height="42dp"
        android:layout_marginStart="208dp"
        android:layout_marginLeft="208dp"
        android:layout_marginTop="60dp"
        android:checked="true"
        android:text="Mata"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/wajah"
        tools:ignore="MissingConstraints" />

    <CheckBox
        android:id="@+id/cbjanggut"
        android:layout_width="115dp"
        android:layout_height="42dp"
        android:layout_marginStart="208dp"
        android:layout_marginLeft="208dp"
        android:layout_marginTop="40dp"
        android:checked="true"
        android:text="Janggut"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cbmata"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>
